<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.github.slick.sample"
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.ExampleActivity"/>
        <activity android:name=".activity.dagger.ExampleActivity"/>
        <activity android:name=".conductor.ConductorActivity"/>
        <activity android:name=".conductor.dagger.ConductorActivity"/>
        <activity android:name=".fragment.FragmentHostActivity"/>
        <activity android:name=".fragmentsupport.SupportFragmentHostActivity"/>
        <activity android:name=".cutstomview.CustomViewActivity">
        </activity>
    </application>

</manifest>